import React from "react";
import { graphql } from "gatsby";
import Layout from "../components/Layout";

const Blog = ({ data }) => (
  <Layout>
	<h1>Blog</h1>
	{ console.log(data) }
  </Layout>
);

export default Blog;
export const AllBlogsQuery = graphql`
  query AllBlogPosts {
    allMarkdownRemark {
      edges {
        node {
          frontmatter {
            date
            title
            description
            author
            path
          }
        }
      }
    }
  }
`
